<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="DarkBaseBrush" Color="#08CFA5" />
    <Color x:Key="DarkBaseColor">#158150</Color>

    <SolidColorBrush x:Key="MediumBaseBrush" Color="#05D2A6" />
    <Color x:Key="MediumBaseColor">#05D2A6</Color>

    <SolidColorBrush x:Key="LightBaseBrush" Color="#05E0B1" />
    <Color x:Key="LightBaseColor">#FF2CB679</Color>

    <SolidColorBrush x:Key="UltraLightBaseBrush" Color="#00EFBC" />
    <Color x:Key="UltraLightBaseColor">#FF3BC588</Color>


    <SolidColorBrush x:Key="DarkBrush" Color="#FFD6D6D6" />
    <Color x:Key="DarkColor">#FFD6D6D6</Color>

    <SolidColorBrush x:Key="MediumBrush" Color="#FFF1F1F1" />
    <Color x:Key="MediumColor">#FFF1F1F1</Color>

    <SolidColorBrush x:Key="LightBrush" Color="white" />
    <Color x:Key="LightColor">white</Color>

    <SolidColorBrush x:Key="TransparentBlackBack" Color="#F2181A24"/>
</ResourceDictionary>